{
  "extends": ["eslint:recommended", "prettier", "prettier/react"],
  "plugins": ["jest"],
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
